    private void jbuttonResetActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // clear and reset text to empty on each value
        jTextFieldCents.setText("");
        jTextFieldQuarters.setText("");
        jTextFieldDimes.setText("");
        jTextFieldNickels.setText("");
        jTextFieldPennies.setText("");
        jTextFieldCents.requestFocusInWindow();
    }                                            

    private void jTextFieldCentsActionPerformed(java.awt.event.ActionEvent evt) {                                                
        // read the number of cents the user has
        
    }                                               

    private void jbuttonCalcActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // declare variables
        int cents, quarters, dimes, nickels, pennies;
        // convert text entered into numberic values
        cents = Integer.parseInt(jTextFieldCents.getText());
         // compute the number of quarters
        quarters=cents/25;
        // compute the remainder
        cents%=25;  // same as cents = cents%25
        // compute the number of dimes
        dimes=cents/10;
        // compute the remainder
        cents%=10;
        //compute the number of nickels
        nickels=cents/5;
        // compute the remainder
        cents%=5;
        // set the remainder to equal the number of pennies
        pennies=cents;
        // display the number of each coin
        jTextFieldQuarters.setText(Integer.toString(quarters));
        jTextFieldDimes.setText(Integer.toString(dimes));
        jTextFieldNickels.setText(Integer.toString(nickels));
        jTextFieldPennies.setText(Integer.toString(pennies));
    }                                           

    private void jbuttonExitActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // Close the current window
        System.exit(0);
    }                                           

